{
  "name": "@mcp-process-to-doc/prompt-change-detection",
  "version": "1.0.0",
  "description": "Prompt-to-code integration system for MCP documentation generation",
  "main": "src/index.js",
  "bin": {
    "analyze-prompt-changes": "./src/cli/analyze-prompt-changes.js",
    "apply-prompt-updates": "./src/cli/apply-prompt-updates.js",
    "validate-integration": "./src/cli/validate-integration.js"
  },
  "scripts": {
    "analyze": "node src/cli/analyze-prompt-changes.js",
    "apply": "node src/cli/apply-prompt-updates.js",
    "validate": "node src/cli/validate-integration.js",
    "sync": "npm run analyze && npm run apply",
    "full-validation": "npm run validate"
  },
  "keywords": [
    "mcp",
    "documentation",
    "prompt-to-code",
    "automation",
    "azure"
  ],
  "author": "MCP Documentation Team",
  "license": "MIT",
  "dependencies": {
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
