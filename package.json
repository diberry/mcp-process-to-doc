{
  "name": "mcp-process-to-doc",
  "version": "1.0.0",
  "description": "Tools to generate documentation for Azure MCP tools",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "generate-tools": "node src/generate-tools-json.js",
    "test": "node src/test-tools-json.js",
    "test-enhanced-content": "node src/test-enhanced-content-generation.js",
    "test-enhanced-docs": "node src/enhanced-documentation-generator.js",
    "test-content-validator": "node src/test-content-validator.js",
    "test-format-checker": "node src/test-format-checker.js",
    "test-link-validator": "node src/test-link-validator.js",
    "test-consistency-checker": "node src/test-consistency-checker.js",
    "test-reference-validator": "node src/test-reference-validator.js",
    "test-all-quality": "npm run test-content-validator && npm run test-format-checker && npm run test-link-validator && npm run test-consistency-checker && npm run test-reference-validator",
    "create-new-md": "node src/create-new-md.js",
    "generate-tool-doc": "node src/generate-tool-doc.js",
    "generate-all-docs": "node src/generate-all-docs.js",
    "update-toc": "node src/navigation-generators/update-toc.js",
    "update-index": "node src/navigation-generators/update-index.js",
    "update-supported-services": "node src/navigation-generators/update-supported-services.js",
    "run-all": "bash src/run-all.sh",
    "full-process": "npm run run-all",
    "analyze-prompt-changes": "node src/automation/cli/analyze-prompt-changes.js",
    "apply-prompt-updates": "node src/automation/cli/apply-prompt-updates.js",
    "validate-integration": "node src/automation/cli/validate-integration.js",
    "sync-prompt-to-code": "npm run analyze-prompt-changes && npm run apply-prompt-updates",
    "full-validation": "npm run validate-integration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diberry/mcp-process-to-doc.git"
  },
  "keywords": [
    "azure",
    "mcp",
    "documentation",
    "generator"
  ],
  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diberry/mcp-process-to-doc/issues"
  },
  "homepage": "https://github.com/diberry/mcp-process-to-doc#readme",
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7"
  }
}
